{
  "name": "react-app-kit",
  "version": "0.0.0",
  "description": "Generic composed app-level components for React apps",
  "main": "lib/react-app-kit.js",
  "module": "lib/react-app-kit.esm.js",
  "jsnext:main": "lib/react-app-kit.esm.js",
  "unpkg": "dist/react-app-kit.min.js",
  "jsdelivr": "dist/react-app-kit.min.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf dist lib",
    "lint": "standard",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production rollup -c",
    "watch": "rollup -c -w",
    "size": "size-limit",
    "_prepublishOnly": "npm run test && npm run size && npm run build",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Matt Styles",
  "license": "MIT",
  "keywords": [
    "design-system",
    "design-tokens",
    "styled-system",
    "react",
    "ui",
    "system-ui",
    "components"
  ],
  "side_effects": false,
  "size-limit": [
    {
      "path": "lib/react-app-kit.js",
      "limit": "60 KB"
    }
  ],
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@size-limit/preset-small-lib": "^2.2.3",
    "@storybook/addon-actions": "^5.3.7",
    "@storybook/addon-backgrounds": "^5.3.7",
    "@storybook/addon-docs": "^5.3.7",
    "@storybook/addon-knobs": "^5.3.7",
    "@storybook/addon-links": "^5.3.7",
    "@storybook/addon-viewport": "^5.3.7",
    "@storybook/react": "^5.3.7",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.10.6",
    "babel-plugin-transform-react-require": "^1.0.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons": "^3.8.0",
    "react-is": "^16.12.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-terser": "^5.1.3",
    "standard": "^14.3.1",
    "styled-components": "^4.x"
  },
  "dependencies": {
    "@reach/menu-button": "^0.8.6",
    "@reach/portal": "^0.8.6",
    "@reach/tooltip": "^0.8.6",
    "@styled-system/css": "^5.1.5",
    "@styled-system/prop-types": "^5.1.5",
    "@styled-system/theme-get": "^5.1.2",
    "basic-kit-theme": "^0.12.0",
    "classnames": "^2.2.6",
    "lodash.merge": "^4.6.2",
    "prop-types": "^15.7.2",
    "raid-streams": "^4.1.0",
    "react-basic-kit": "^0.12.0",
    "react-kit-core": "^0.12.0",
    "react-markdown": "^4.3.1",
    "styled-system": "^5.0.15"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "styled-components": "^4.x"
  }
}
