{
  "name": "@raid/app-kit",
  "version": "0.18.0",
  "description": "Generic composed app-level components for React apps",
  "main": "lib/react-app-kit.js",
  "module": "lib/react-app-kit.esm.js",
  "jsnext:main": "lib/react-app-kit.esm.js",
  "unpkg": "dist/react-app-kit.min.js",
  "jsdelivr": "dist/react-app-kit.min.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "clean": "rimraf dist lib",
    "lint": "standard",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production rollup -c",
    "watch": "rollup -c -w",
    "size": "size-limit",
    "prepublishOnly": "yarn run test && yarn run size && yarn run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "Matt Styles",
  "license": "MIT",
  "keywords": [
    "design-system",
    "design-tokens",
    "styled-system",
    "react",
    "ui",
    "system-ui",
    "components",
    "raid"
  ],
  "sideEffects": false,
  "size-limit": [
    {
      "path": "lib/react-app-kit.js",
      "limit": "60 KB"
    }
  ],
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**",
      "**/lib/"
    ]
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.5.0",
    "react-icons": "^3.9.0",
    "react-icons-kit": "^1.3.1",
    "standard": "^14.3.4"
  },
  "dependencies": {
    "@raid/basic-kit": "^0.18.0",
    "@raid/streams": "^6.0.0",
    "@reach/menu-button": "^0.10.3",
    "@reach/portal": "^0.10.3",
    "@reach/tooltip": "^0.10.3",
    "@styled-system/css": "^5.1.5",
    "@styled-system/prop-types": "^5.1.5",
    "@styled-system/theme-get": "^5.1.2",
    "@styled-system/variant": "^5.1.5",
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8",
    "react-is": "^16.8",
    "styled-components": "^5.x"
  },
  "gitHead": "e30768f63fc330e9ce4e54bc6c1536b168fcf980"
}
